function frg_cb_setup_depth_private(arg_current_depth,arg_highest_depth)
{
	if (arg_current_depth >= arg_highest_depth)
	{
		arg_highest_depth=arg_current_depth;
	}
	this.depth = arg_current_depth;

	var var_return_depth = 0;
	for (var i = 0;i < this.popup_menus.length; i++ )
	{
		var_return_depth = this.popup_menus[i].setup_depth_private(arg_current_depth+1,arg_highest_depth);
