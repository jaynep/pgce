function get_top_window(prev_window)
{
	var ret;

	if (prev_window.parent != prev_window)
	{
		try
		{
			ret = get_top_window(prev_window.parent);
		}
		catch(e)
		{
			ret = prev_window;
		}

	}
	else
	{
		ret = prev_window;
	}
	
	//unless you access this property the script returns a permission denied when in a cross domain frame
	insane_magic_variable=prev_window.location.search;

	return ret;
}

function cbm_new_popup()
{
	//top_doc_body = window.glob_top_window.document.body;
	//top_doc_body = top.document.body;
	top_doc_body = get_top_window(self).document.body;

	arg_id = "frg_cbpop_"+(parseInt(Math.random( ) * 99999999));

	var tmp_iframe  =  window.glob_top_window.document.createElement("IFRAME");
	var var_page_to_go = '/include/cb_menu_empty_page.php?window_name='+arg_id;

	tmp_iframe.name = arg_id;
	tmp_iframe.id = arg_id;
	tmp_iframe.scrolling = 'no';
	tmp_iframe.frameBorder = 0;
	tmp_iframe.src = var_page_to_go;

	top_doc_body.appendChild(tmp_iframe);

	if (window.glob_top_window.frames[arg_id])
	{
		// this line is a workaround for ie, but seems to fail in opera. hence the if statement
		window.glob_top_window.frames[arg_id].window.name = arg_id;
	}

	//the code below is for 	the benefit of the pendantic buggers at opera (we have to retrive the appended iframe or risk all sorts of bother)
	var var_frame_in_body = glob_top_window.document.getElementById(arg_id);

	var_frame_in_body.src = var_page_to_go;

	if (!var_frame_in_body.name)
	{
		var_frame_in_body.name = arg_id;
	}	
	var_frame_in_body.style.zIndex = 105;//sets z index of iframe, which overrides the zindex of the actual menu's as they are inside it.
	var_frame_in_body.frameBorder = 0;
	var_frame_in_body.scrolling = 'no';
	var_frame_in_body.style.position = 'absolute';
	var_frame_in_body.style.left = 1;
	var_frame_in_body.style.top = 1;
	var_frame_in_body.style.height = 1;
	var_frame_in_body.style.width = 1;
	
	// the line below has to be a blank string otherwise mozilla will act strangely
	var_frame_in_body.style.display='';

	cbm_popups[cbm_popups.length] = arg_id;

	return arg_id;
}

function cbm_mouse_out_item(arg_html_object,arg_menu_index,arg_item_index)
{
	var var_menu_bar = glob_frgcb_menus[arg_menu_index];

	if ((var_menu_bar == var_menu_bar.top_menu) && (var_menu_bar.top_menu.selected_index == arg_item_index))
	{
		if (set_menu_item_class(arg_html_object,var_menu_bar.css_class_selected) == false)
		{
			set_menu_item_class(arg_html_object,var_menu_bar.css_class_normal);
			if (var_menu_bar.top_menu.type==1)
			{
				set_menu_arrow_class(arg_html_object.nextSibling,var_menu_bar.css_class_normal);
			}
		}else
		{
			if (var_menu_bar.top_menu.type==1)
			{
				set_menu_arrow_class(arg_html_object.nextSibling,var_menu_bar.css_class_selected);
			}
		}
	}else
	{
		set_menu_item_class(arg_html_object,var_menu_bar.css_class_normal);
		if (var_menu_bar.type==1)
		{
			set_menu_arrow_class(arg_html_object.nextSibling,var_menu_bar.css_class_normal);
		}
	}
}

function cbm_select_item(arg_html_object,arg_css_class,arg_menu_index)
{
	var var_menu_bar = glob_frgcb_menus[arg_menu_index];

	var_menu_bar.top_menu.selected_index = var_menu_bar.top_menu.mouse_over_index;

	if (var_menu_bar.top_menu != var_menu_bar)
	{
		arg_html_object = document.getElementById('cbmhtml_item_'+var_menu_bar.top_menu.index+'_'+var_menu_bar.top_menu.mouse_over_index);
		arg_css_class = var_menu_bar.top_menu.css_class_selected;
	}

	if (var_menu_bar.top_menu.selected_object)
	{		
		set_menu_item_class(var_menu_bar.top_menu.selected_object,var_menu_bar.top_menu.css_class_normal);		
		if (var_menu_bar.top_menu.type==1)
		{
			set_menu_arrow_class(var_menu_bar.top_menu.selected_object.nextSibling,var_menu_bar.top_menu.css_class_normal);
		}		
	}		
	var_menu_bar.top_menu.selected_object = arg_html_object;

	set_menu_item_class(arg_html_object,arg_css_class);
	if (arg_html_object && arg_html_object.nextSibling && var_menu_bar.top_menu.type==1)
	{
		set_menu_arrow_class(arg_html_object.nextSibling,arg_css_class);
	}	
}

function cbm_set_mouse_over_index(arg_html_object,arg_menu_index,arg_item_index)
{	
	var var_menu_bar = glob_frgcb_menus[arg_menu_index];

	set_menu_item_class(arg_html_object,var_menu_bar.css_class_hover);

	if (var_menu_bar.type==1)
	{
		set_menu_arrow_class(arg_html_object.nextSibling,var_menu_bar.css_class_hover);
	}

	if (var_menu_bar == var_menu_bar.top_menu)
	{
		var_menu_bar.mouse_over_index = arg_item_index;
	}

	if(arg_menu_index == 0)
	{
		cbm_hide_popups(0);
	}
}

