		function getURL(s_Type,s_LinkVar){

			var var_return_page = "";

			if((s_Type=="i") || (s_Type=="internal")){
			
				var temp_value = s_LinkVar.split("|");
				//var_return_page= location.protocol+"//"+location.host+"/index.phtml?d="+temp_value[0];
				if((temp_value[0].length > 0) && (temp_value[0] != "none"))
				{
					var_return_page= "/?d="+temp_value[0];
				}
			
			}else if((s_Type=="e") || (s_Type=="external")){
			
				var temp_value = s_LinkVar.split("|");
				if((temp_value[0].length > 0) && (temp_value[0] != "none"))
				{
					var_return_page= temp_value[0];
				}
		
			}else{
				var_return_page= s_LinkVar;
			}
			
			return var_return_page;
			
		
		}


		function linkhandler_(n_Catagory,n_Item){
			
		
			var_location = getURL(Menu_.Catagory[n_Catagory].Items[n_Item].LinkType,Menu_.Catagory[n_Catagory].Items[n_Item].LinkVar);
			nav_target = Menu_.Catagory[n_Catagory].Items[n_Item].Target;
			nav_url = var_location;
			nav_type = Menu_.Catagory[n_Catagory].Items[n_Item].LinkType;
			nav_value = Menu_.Catagory[n_Catagory].Items[n_Item].LinkVar;

			if(nav_target == 'current')
			{
				nav_target = 'current_crystal';
			}

			if ((nav_url == 'none' ) || (nav_value == 'none' ))
			{
					// do nothing
			}
			else if (nav_type == 'i')
			{
				temp_value = nav_value.split('|');
				if ((temp_value[0] == 'none') || (temp_value[0] == ''))
				{
					// do nothing
				}else
