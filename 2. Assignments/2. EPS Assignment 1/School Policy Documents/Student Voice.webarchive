		// Javascript Function to find the position of the iFrame we are trying to EDIT. 'obj' is the name of the AJAX DIV
		function find_qe_iframe(obj)
		{
//--------------- As a start, we get the Top and Left offsets with the browser and the URL on the address bar
			div_top = obj.offsetTop;
			div_left = obj.offsetLeft; frame_width = 0; frame_height = 0;
			var myURL = "location.href";
//--------------- The Div is in an iFrame? how many iFrame nodes? we locate that comparing "parents" until they are equal
			var conta = 0;
			while (eval(myURL) != eval("parent."+myURL)) { myURL = "parent."+myURL; conta++; }
//--------------- "newparent" will be the final parenting expression. Example: parent.parent.parent.document
			var newparent = "";
//--------------- FrameLocation is an array with the iFrame objects between our AJAX DIV and the ultimate parent document
			var FrameLocation = new Array();
			var fl_x = 0;
//--------------- The first layer is "document" and then we add "parent." to other layers on the way to the AJAX DIV
			FrameLocation[fl_x]="document";
//--------------- If we Edit the main page we will have no disadjustments
			var disadjustment = 0;
//--------------- "conta" has the number of iFrames between the document and the iFrame, so lets sniff the address to it
			for(j=0;j<conta;j++)
			{
//--------------- If we are on an iFrame, we will have a 4px disadjustment that will be fixed later
				disadjustment = 1;
//--------------- We apply "newparent" to the location for recursivity tasks
				var myURL = newparent+"location.href";
//--------------- We apply "newparent" to the parent location to compare with the following parent
				var parentURL = newparent+"parent.location.href";
//--------------- This pair of URL are checked, so we add a new parent to the "newparent" variable -> Example: parent.parent.
				newparent = "parent."+newparent;
//--------------- If the actual URL and parent URL are different, we are still on an iFrame!!
				if (eval(myURL) != eval(parentURL))
				{
//--------------- For recursivity matters, we need to create text variables with new root and use eval() to work with them
					var has_frames = newparent+"window.frames.length";
//--------------- If has frames, we need to find which of them leads to our AJAX DIV
					if (eval(has_frames) != 0)
					{
						for(i=0;i<eval(has_frames);i++)
						{
//--------------- We get the URL of the parent of the iFrame to see if is our URL
							var frameURL = newparent+"window.frames["+i+"].location";
//--------------- If the URLs are the same, we are on the path to the AJAX DIV
							if (eval(frameURL) == eval(myURL))
							{
//--------------- We get the name of the iFrame to add it to the FrameLocation array that leads to the AJAX DIV
								var frameName = newparent+"window.frames["+i+"].name";
								if (document.all) // Internet Explorer
								{
									frame_width = eval(newparent+"window.frames["+i+"].window.document.body.offsetWidth");
									frame_height = eval(newparent+"window.frames["+i+"].window.document.body.offsetHeight");
								}
								else
								{
									frame_width = eval(newparent+"window.frames["+i+"].innerWidth");
									frame_height = eval(newparent+"window.frames["+i+"].innerHeight");
								}
//--------------- We add it to the array. Example: [0]"document" -> [1]".getElementById("ext_2_2").contentWindow.document"
								fl_x++;
								FrameLocation[fl_x] = ".getElementById(\""+eval(frameName)+"\").contentWindow.document";
//--------------- We locate the iFrame to sum the Top and Left offsets
								myFrame = eval(frameName);
								var xFrame = newparent+"document.getElementById(myFrame)";
								element = eval(xFrame);
//--------------- We call external functions to get the Top and Left offsets
								div_top+= get_qe_top(element);
								div_left+= get_qe_left(element);
							}
						}
					}
				}
			}
//--------------- Fixing the iFrame disadjustment to display the "Canvas" on the Quick Edit Toolkit
			if (disadjustment > 0)
			{
				div_top-=4;
				div_left-=4;
			}
//--------------- We are going to create the correct path to the AJAX DIV to be sent to the Quick Edit
			FrameTemp = "document";
//--------------- if we have iFrames, we have to convert the path reverting the FrameLocation array
			if (conta > 0)
			{
				for(i=(FrameLocation.length-1);i>0;i--) {FrameTemp+=FrameLocation[i];}
//--------------- This is not very nice, but is to erase the LAST ".contentWindow.document" that we dont need
				FrameTemp+="_to_erase";
				FrameTemp=FrameTemp.replace(/.contentWindow.document_to_erase/,"");
			}
//--------------- We return the total Top and Left offsets, the "newparent" address and the iFrame path
			return div_top+":"+div_left+":"+newparent+":"+FrameTemp+":"+frame_width+":"+frame_height;
		}

