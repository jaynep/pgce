{
	if(obj.parentNode.className == "frog_scroller_class")
	{
		obj = obj.parentNode;
	}
}

var objposition = findPos(obj,1);
var buttonposition = findPos(button,0);
outlineobj.style.top=objposition[1] - 3;
outlineobj.style.left=objposition[0] - 3;
outlineobj.style.width=objposition[2] + 3;
outlineobj.style.height=objposition[3] - buttonposition[3] + 3;

var outlinebottom = objposition[1] + objposition[3] - buttonposition[3];
var buttontop = buttonposition[1];

if(outlinebottom > buttontop)
{
	var x = outlinebottom - buttontop;
	outlineobj.style.height = parseInt(outlineobj.style.height) - x;
}

outlineobj.style.borderStyle='dashed';
document.body.appendChild(outlineobj);
 }
