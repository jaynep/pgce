		function __ScaleX_(n_X){
		
			if(Scrolly_.ScrollBarWidth==Handy_.SliderHandleWidth)
			{
				return 0;
			}
		
			var sliderY = (n_X - 21) / ((Scrolly_.ScrollBarWidth-Handy_.SliderHandleWidth) / 100)
				
			var sliderX = sliderY * (((Slidey_.SlidingPanelWidth - Scrolly_.ViewportWidth) / 100))
			
			return sliderX;
		
		}
		
		
