/***********utility functions*************************/
function frgcb_findframe(arg_name, arg_search_window)
{
	if (!arg_name)
	{
		return false;
	}
	
	if (arg_search_window.frames[arg_name])
	{
		return ".frames['"+escape(arg_name)+"']";
	}
	try
	{
		for (var i =0;i < arg_search_window.frames.length; i++)
		{		 
			if (arg_search_window.frames[i].name == arg_name)
			{
				return ".frames["+i+"]";
			}else
			{
				var return_frame = this.findframe(arg_name, arg_search_window.frames[i]);
				if (return_frame)
				{
					return ".frames["+i+"]"+return_frame;
				}
			}
		}
	}
	catch(e) 
	{
//		alert(e.description);
	}

	return false;
}

