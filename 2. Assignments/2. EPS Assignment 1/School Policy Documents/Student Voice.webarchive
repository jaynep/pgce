function get_top_window(prev_window)
{
	var ret;

	if (prev_window.parent != prev_window)
	{
		try
		{
			ret = get_top_window(prev_window.parent);
		}
		catch(e)
		{
			ret = prev_window;
		}

	}
	else
	{
		ret = prev_window;
	}
	
	//unless you access this property the script returns a permission denied when in a cross domain frame
	insane_magic_variable=prev_window.location.search;

	return ret;
}

function cbm_new_popup()
{
	//top_doc_body = window.glob_top_window.document.body;
	//top_doc_body = top.document.body;
	top_doc_body = get_top_window(self).document.body;

	arg_id = "frg_cbpop_"+(parseInt(Math.random( ) * 99999999));

	var tmp_iframe  =  window.glob_top_window.document.createElement("IFRAME");
	var var_page_to_go = '/include/cb_menu_empty_page.php?window_name='+arg_id;

	tmp_iframe.name = arg_id;
	tmp_iframe.id = arg_id;
	tmp_iframe.scrolling = 'no';
	tmp_iframe.frameBorder = 0;
	tmp_iframe.src = var_page_to_go;

	top_doc_body.appendChild(tmp_iframe);

	if (window.glob_top_window.frames[arg_id])
	{
		// this line is a workaround for ie, but seems to fail in opera. hence the if statement
		window.glob_top_window.frames[arg_id].window.name = arg_id;
	}

	//the code below is for 	the benefit of the pendantic buggers at opera (we have to retrive the appended iframe or risk all sorts of bother)
	var var_frame_in_body = glob_top_window.document.getElementById(arg_id);

	var_frame_in_body.src = var_page_to_go;

	if (!var_frame_in_body.name)
	{
		var_frame_in_body.name = arg_id;
	}	
	var_frame_in_body.style.zIndex = 105;//sets z index of iframe, which overrides the zindex of the actual menu's as they are inside it.
	var_frame_in_body.frameBorder = 0;
