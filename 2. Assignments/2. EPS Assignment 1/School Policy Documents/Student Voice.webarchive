		function __TIMER_PanelDragged_Update_(n_Milliseconds){
		
			var temp = new Date();
			temp = temp.getTime();
		
			var MillisecondsElapsed = temp-n_Milliseconds;
			
			var offset = 1;

			if(MillisecondsElapsed > 0 && MillisecondsElapsed < 20){
			
				offset = 10;
			
			}else if (MillisecondsElapsed >= 20  && MillisecondsElapsed < 40){
			
				offset = 8;
			
			}else if (MillisecondsElapsed >= 40  && MillisecondsElapsed < 60){
			
				offset = 6;
			
			}else if (MillisecondsElapsed >= 60  && MillisecondsElapsed < 80){
			
				offset = 4;
			
			}else if (MillisecondsElapsed >= 100){
			
				offset = 2;
			
			}
			
			Slidey_.setTargetX(Slidey_.ScaleX(Handy_.CurrentX));
			
			if(Slidey_.CurrentX != Slidey_.TargetX){
			
			var PixelsToMove = Math.round(((Slidey_.TargetX-Slidey_.CurrentX) / offset));
			Slidey_.CurrentX+= PixelsToMove;
		
			Slidey_.Draw();
			
			}
			
			
		
			Slidey_.FreeTimeoutID = setTimeout("__TIMER_PanelDragged_Update_("+temp+")",10);
		
		}
		
