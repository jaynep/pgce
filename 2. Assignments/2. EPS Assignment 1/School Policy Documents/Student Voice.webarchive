//get the topmost window that we can use. this eliminates the problem of having a page running in another domain
function find_top_window(var_window)
{
	try
	{
		if (var_window.parent.document)
		{
			if (var_window.parent == var_window || var_window.this_is_top_window)
			{
				return var_window;
			}
			else
			{
				return find_top_window(var_window.parent);
			}
		}
	}catch(e)
	{
		return var_window;
	}
}

