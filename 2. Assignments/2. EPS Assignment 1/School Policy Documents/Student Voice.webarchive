		function __SetCatagoryWidths_(n_BarWidth){

			var PixelsEach = Math.floor(n_BarWidth / this.NumCatagories);
			var Accumulator = 0;

			for(i=0;i<this.NumCatagories;i++)
			{
				
				this.Catagory[i].CatagoryTotalWidth =  PixelsEach;
				Accumulator+= this.Catagory[i].CatagoryTotalWidth;
		
				document.getElementById("ScrollMiddleAlpha__Cat"+i).style.width=this.Catagory[i].CatagoryTotalWidth+"px";
				document.getElementById("ScrollMiddleOverlay__Cat"+i).style.width=this.Catagory[i].CatagoryTotalWidth+"px";
			}
			
			var Remainder= n_BarWidth-Accumulator;
			
			
			this.Catagory[0].CatagoryTotalWidth+=Remainder;
			
			for(i=0;i<this.NumCatagories;i++)
			{
				//alert("ScrollMiddleOverlay__Cat"+i);
				document.getElementById("ScrollMiddleAlpha__Cat"+i).style.width=this.Catagory[i].CatagoryTotalWidth+"px";
				document.getElementById("ScrollMiddleOverlay__Cat"+i).style.width=this.Catagory[i].CatagoryTotalWidth+"px";
			}

			/* Fixes to stop Windows 2000/IE crashes caused by floats if the width goes outside a container */

			var minus = 0;

			if (browser_.isWin2000)
			{
				minus = (this.NumCatagories + 1);

				// This line removes the shiny image from the whole of the scroll bar - without it, only the last section has no shiny
				document.getElementById("ScrollMiddle_").style.background="none";
				document.getElementById("ScrollMiddleOverlay__Cat"+(this.NumCatagories-1)).style.backgroundColor=document.getElementById("ScrollMiddleAlpha__Cat"+(this.NumCatagories-1)).style.backgroundColor;
			}
			document.getElementById("ScrollMiddleAlpha__Cat"+(this.NumCatagories-1)).style.width=(this.Catagory[(this.NumCatagories-1)].CatagoryTotalWidth - minus)+"px";
			document.getElementById("ScrollMiddleOverlay__Cat"+(this.NumCatagories-1)).style.width=(this.Catagory[(this.NumCatagories-1)].CatagoryTotalWidth)+"px";
			
			WriteDebug(" FFHack "+GLOBAL_UglyFireFoxHack+" NumCats = "+this.NumCatagories+", TotalItems="+this.TotalItems+" PixelsEach="+PixelsEach+", Accumulator: "+Accumulator+", Viewport: "+Scrolly_.ViewPortWidth+", n_BarWidth="+n_BarWidth+", ScrolCont="+parseInt(document.getElementById("ScrollBarContainer_").offsetWidth)+"<br>");
			
	


		}
		
		
