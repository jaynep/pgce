}

function cbm_mouse_out_item(arg_html_object,arg_menu_index,arg_item_index)
{
	var var_menu_bar = glob_frgcb_menus[arg_menu_index];

	if ((var_menu_bar == var_menu_bar.top_menu) && (var_menu_bar.top_menu.selected_index == arg_item_index))
	{
		if (set_menu_item_class(arg_html_object,var_menu_bar.css_class_selected) == false)
		{
			set_menu_item_class(arg_html_object,var_menu_bar.css_class_normal);
			if (var_menu_bar.top_menu.type==1)
			{
				set_menu_arrow_class(arg_html_object.nextSibling,var_menu_bar.css_class_normal);
			}
		}else
		{
			if (var_menu_bar.top_menu.type==1)
			{
				set_menu_arrow_class(arg_html_object.nextSibling,var_menu_bar.css_class_selected);
			}
		}
	}else
	{
		set_menu_item_class(arg_html_object,var_menu_bar.css_class_normal);
		if (var_menu_bar.type==1)
		{
			set_menu_arrow_class(arg_html_object.nextSibling,var_menu_bar.css_class_normal);
		}
	}
}

function cbm_select_item(arg_html_object,arg_css_class,arg_menu_index)
{
	var var_menu_bar = glob_frgcb_menus[arg_menu_index];

	var_menu_bar.top_menu.selected_index = var_menu_bar.top_menu.mouse_over_index;

	if (var_menu_bar.top_menu != var_menu_bar)
	{
		arg_html_object = document.getElementById('cbmhtml_item_'+var_menu_bar.top_menu.index+'_'+var_menu_bar.top_menu.mouse_over_index);
		arg_css_class = var_menu_bar.top_menu.css_class_selected;
	}

	if (var_menu_bar.top_menu.selected_object)
	{		
		set_menu_item_class(var_menu_bar.top_menu.selected_object,var_menu_bar.top_menu.css_class_normal);		
		if (var_menu_bar.top_menu.type==1)
		{
			set_menu_arrow_class(var_menu_bar.top_menu.selected_object.nextSibling,var_menu_bar.top_menu.css_class_normal);
		}		
	}		
	var_menu_bar.top_menu.selected_object = arg_html_object;

	set_menu_item_class(arg_html_object,arg_css_class);
	if (arg_html_object && arg_html_object.nextSibling && var_menu_bar.top_menu.type==1)
	{
		set_menu_arrow_class(arg_html_object.nextSibling,arg_css_class);
	}	
}

function cbm_set_mouse_over_index(arg_html_object,arg_menu_index,arg_item_index)
{	
	var var_menu_bar = glob_frgcb_menus[arg_menu_index];

	set_menu_item_class(arg_html_object,var_menu_bar.css_class_hover);

	if (var_menu_bar.type==1)
	{
		set_menu_arrow_class(arg_html_object.nextSibling,var_menu_bar.css_class_hover);
	}

	if (var_menu_bar == var_menu_bar.top_menu)
	{
		var_menu_bar.mouse_over_index = arg_item_index;
	}

	if(arg_menu_index == 0)
	{
		cbm_hide_popups(0);
	}
}

