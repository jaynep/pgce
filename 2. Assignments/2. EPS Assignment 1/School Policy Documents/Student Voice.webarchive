			var x;
		
			if (id)
			
				dragObj_.elNode = document.getElementById(id);
		  
			else {
		   
		
			if (browser_.isIE)
				
				dragObj_.elNode = window.event.srcElement;
			
			if (browser_.isNS)
			  
				dragObj_.elNode = event.target;
		
			if (dragObj_.elNode.nodeType == 3)
			  
				dragObj_.elNode = dragObj_.elNode.parentNode;
				
			}
			
			if (event.target)
			{
				targ=event.target;
			}
			else if (event.srcElement)
			{
				targ=event.srcElement;
			}
			
		
				if (browser_.isIE) {
				
				x = window.event.clientX + document.documentElement.scrollLeft
				  + document.body.scrollLeft;
				
				}
				
				if (browser_.isNS) {
					
					x = event.clientX + window.scrollX;
			
				}
			
				dragObj_.cursorStartX = x;
			
				dragObj_.elStartLeft  = parseInt(dragObj_.elNode.style.left, 10);
			
				if (isNaN(dragObj_.elStartLeft)) dragObj_.elStartLeft = 0;
			
				if(targ.id=="SliderHandle_" || targ.id=="SliderHandleMiddle_"){
				
					Slidey_.StartX = Slidey_.ScaleX(Handy_.CurrentX+10);
					Slidey_.ReadyToDrag = true;
			
				if (browser_.isIE) {
				
					document.attachEvent("onmousemove", dragGo_);
					document.attachEvent("onmouseup",   dragStop_);
					window.event.cancelBubble = true;
					window.event.returnValue = false;
				}
			  
				if (browser_.isNS) {
					
					document.addEventListener("mousemove", dragGo_,   true);
					document.addEventListener("mouseup",   dragStop_, true);
					event.preventDefault();
			  
				}
			
			}else{
			
			Handy_.StartX = Handy_.CurrentX;
		
			Handy_.setTarget((dragObj_.cursorStartX-10)-(Handy_.SliderHandleWidth/2));
			
			Handy_.Init(1000);
			
			Handy_.TimeoutID = setTimeout("__TIMER_Slide_NextFrame_()",10);
			
			}
		
		}
		
		
		function dragGo_(event) {
		
			var x; 
		
			if (browser_.isIE) {
			
				x = window.event.clientX + document.documentElement.scrollLeft
				+ document.body.scrollLeft;
		
			}
			if (browser_.isNS) {
			
					x = event.clientX + window.scrollX;
			
			}
		
			Handy_.CurrentX = (dragObj_.elStartLeft + x - dragObj_.cursorStartX);
			Handy_.Draw();
		
		
			if(Slidey_.ReadyToDrag == true){
			Slidey_.setTargetX(Slidey_.ScaleX(Handy_.CurrentX));
				
			}else{
				Slidey_.setTargetX(Slidey_.ScaleX(Handy_.CurrentX));
			}
		
			if (browser_.isIE) {
			
				window.event.cancelBubble = true;
				window.event.returnValue = false;
				
			}
				if (browser_.isNS)
				
				event.preventDefault();
		}
		
		
		function dragStop_(event) {
			
			Slidey_.ReadyToDrag == false;
			
			if (browser_.isIE) {
			
				document.detachEvent("onmousemove", dragGo_);
				document.detachEvent("onmouseup",   dragStop_);
				
			}
			if (browser_.isNS) {
			
				document.removeEventListener("mousemove", dragGo_,   true);
				document.removeEventListener("mouseup",   dragStop_, true);
				
			}
		}
		
		
		
