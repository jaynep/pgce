function cbm_show_popup(arg_depth,arg_left,arg_top,arg_menu_index,arg_item_index)
{
	// hide all windows below thie popup. this stops a popup belonging to another 
	// menuitem from showing up	
	if(window.glob_top_window)
	{
		for (var i=arg_depth;i <  cbm_popups.length;i++)
		{
			window.glob_top_window.document.getElementById(cbm_popups[i]).style.display = 'none';
		}
	}	
	var var_menu_bar = glob_frgcb_menus[arg_menu_index];
	var var_menitem = var_menu_bar.menuitems[arg_item_index];
	// the above code uses the indexes given to find the menuitem we are currently using

	var var_popupmenu = var_menu_bar.popup_menus[var_menu_bar.popmenu_indexes[var_menitem[0]]];
	// the behemoth above gets the menuitem's corresponding popupmenu

	var var_iframe_id = cbm_popups[arg_depth];
	//var var_iframe = window.glob_top_window.document.getElementById(var_iframe_id);	
	top_doc = get_top_window(self);
	var var_iframe = top_doc.document.getElementById(var_iframe_id);
//FIXME: the empty menu safari bug happens above.
// for some reason the browser can't find the frame it needs	

	if (arg_depth < cbm_popups.length)
	{
		
		if (typeof(var_popupmenu) == 'object')
		{					
			var_iframe.style.display = '';
			var_iframe.style.left = arg_left;
			var_iframe.style.top = arg_top;

			var var_iframe_window = cbm_find_frame(top,var_iframe_id);

			if (var_iframe_window == false)
			{
				var_iframe_window = window.glob_top_window.frames[var_iframe_id];
			}
			
			if (var_iframe_window.document == null)
			{
				glob_top_window.temp_iframe = var_iframe_window;
				glob_top_window.temp_popup_object = var_popupmenu;
				glob_top_window.temp_iframe_object = var_iframe;
				glob_top_window.window.clearTimeout(glob_top_window.temp_iframe_object_timeout);
				//glob_top_window.temp_iframe_object_timeout = glob_top_window.window.setTimeout("cbm_showpopup_for_ns6()",100);
				return false;
			}

			if(!var_iframe_window.document.getElementById('frgcb_menu_div'))
			{
//DEBUG
//alert("no");
				return false;
			}		

			// this section of code is dulpicated in the function cbm_showpopup_for_ns6()
			var var_temp_div = var_iframe_window.document.getElementById('frgcb_menu_div');
			var_popupmenu.create_menu(var_temp_div);
			var var_new_window_width = var_temp_div.offsetWidth;
			var var_new_window_height = var_temp_div.offsetHeight;
//FIXME: explorer 5.2 on the mac fails above, it cannot get the offset width or height of the object

			var_iframe.style.width = var_new_window_width;
			var_iframe.style.height = var_new_window_height;	

		}else
		{
			cbm_hide_popups(arg_depth);
			var_iframe.style.display = 'none';		
		}
	}	
}

function cbm_showpopup_for_ns6()
{
	// this is a bug fix for netscape 6 and it duplicates the end of teh previous function
	// grrrrrrrr
	var var_iframe_window = glob_top_window.temp_iframe;
	var var_popupmenu = glob_top_window.temp_popup_object;
	var var_iframe = glob_top_window.temp_iframe_object;

	if (var_iframe_window.document != null)
	{
		var var_temp_div = var_iframe_window.document.getElementById('frgcb_menu_div');
		
		var_popupmenu.create_menu(var_temp_div);
		var var_new_window_width = var_temp_div.offsetWidth;
		var var_new_window_height = var_temp_div.offsetHeight;
		
		var_iframe.style.width = var_new_window_width;
		var_iframe.style.height = var_new_window_height;	
	}
}

function cbm_find_frame(arg_window,arg_name)
{
	for (var i = 0;i<arg_window.frames.length;i++)
	{
		try
		{
			if (arg_window.frames[i].name == arg_name)
			{				
				return arg_window.frames[i];
			}
		}catch(e)
		{
			//alert(e.description);
			// do nothing
		}
	}
	return false;
}

function cbm_hide_popups(arg_depth)
{
	var var_iframe_id = ''; 

	var i = arg_depth;
	
	while (i < cbm_popups.length)
	{
		var_iframe_id = cbm_popups[i];
		//window.glob_top_window.document.getElementById(var_iframe_id).style.display = 'none';
		if(self == glob_top_window)
		{
			top_doc = self;
		}
		else
		{
			top_doc = get_top_window(self);
		}

		top_doc.document.getElementById(var_iframe_id).style.display = 'none';
		i++;
	}	
}

