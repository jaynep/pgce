function frgcb_apply_styles(arg_css_class_name,arg_html_div,arg_other_properties,arg_type)
{
	var var_menu_table = arg_html_div.firstChild;
	var var_menu_tbody = var_menu_table.firstChild;

	if (arg_type == 1)
	{		
		var var_border1 = 'borderLeft';
		var var_border2 = '';
		var var_border3 = 'borderTop';
		var var_border4 = 'borderBottom';

		var var_border5 =  "";
		var var_border6 =  "borderRight";
		var var_border7 =  "borderTop";
		var var_border8 =  "borderBottom";
		
	}else
	{
		var var_border1 = 'borderTop';
		var var_border2 = 'borderBottom';		
		var var_border3 = 'borderLeft';
		var var_border4 = 'borderRight';
	}

	var var_border_style = (arg_other_properties['border_color']?"solid 1px "+arg_other_properties['border_color']:"0");
	var var_splitter_style = (arg_other_properties['splitter_color']?"solid 1px "+arg_other_properties['splitter_color']:"0");
	var var_menu_rows = var_menu_tbody.childNodes;
	for (var i_r = 0; i_r <  var_menu_rows.length; i_r++ )
	{
		var_menu_cells = var_menu_rows[i_r].childNodes;
		for (var i_c = 0; i_c <  var_menu_cells.length; i_c++ )
		{			
			if (var_menu_cells[i_c].className == 'frg_cb_menu_class')			
			{

				// for each table cell that is not a spacer apply these styles
				// this means that we can have the desired 'cellpadding' effect
				set_menu_item_class(var_menu_cells[i_c],arg_css_class_name);
				if (arg_type == 1) 
				{
					if (var_menu_cells[i_c+1].style.display != 'none')
					{
						set_menu_arrow_class(var_menu_cells[i_c+1],arg_css_class_name);
					}					
				}

				if (i_c == 0 && i_r == 0)
				{
					// apply this to the first cell in the menu bar
					var_menu_cells[i_c].style[var_border3] =  var_border_style;
					if (arg_type == 1) {var_menu_cells[i_c+1].style[var_border7] =  var_border_style;}
				}else
				{
					var_menu_cells[i_c].style[var_border3] =  var_splitter_style;
					if (arg_type == 1) {var_menu_cells[i_c+1].style[var_border7] =  var_splitter_style;}
				}	

				var_menu_cells[i_c].style[var_border1] =  var_border_style;

				//if (arg_type == 1) {var_menu_cells[i_c+1].style[var_border5] =  var_border_style;}

				if (arg_type != 1)
				{
					var_menu_cells[i_c].style[var_border2] =  var_border_style;
				}
				if (arg_type == 1) 
				{
					if (var_menu_cells[i_c+1].style.display == 'none')
					{
						var_menu_cells[i_c].style[var_border6] =  var_border_style;
					}else
					{
						var_menu_cells[i_c+1].style[var_border6] =  var_border_style;
					}
				}

				if ((i_c == var_menu_cells.length-1) || ((i_r == var_menu_rows.length-1) && (arg_type == 1)))
				{
					// apply this to the last cell in the menu bar
					var_menu_cells[i_c].style[var_border4] =  var_border_style;
					if (arg_type == 1) {var_menu_cells[i_c+1].style[var_border8] =  var_border_style;}
				}else if((arg_type == 1) && (arg_other_properties['spaces'] > 0))
				{
					var_menu_cells[i_c].style[var_border4] =  var_splitter_style;
					if (arg_type == 1) {var_menu_cells[i_c+1].style[var_border8] =  var_splitter_style;}
				}
			}
		}
	}
}

