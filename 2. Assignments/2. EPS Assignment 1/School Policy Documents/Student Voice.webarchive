		function __setSliderHandleWidth_(n_ScrollBarWidth){
		

			// What percent (from 0 to 1) is the viewport of the total?
		
			this.Ratio = ((Scrolly_.ViewportWidth / (Slidey_.SlidingPanelWidth / 100)) / 100);

			this.SliderHandleWidth = (Scrolly_.ScrollBarWidth * this.Ratio);
			
			if(this.SliderHandleWidth > n_ScrollBarWidth){
				this.SliderHanleWidth = n_ScrollBarWidth;
			}

			if(this.SliderHandleWidth <= 14)
			{
				this.SliderHandleWidth = 15;
			}
	
			document.getElementById("SliderHandleMiddle_").style.width = (this.SliderHandleWidth - 14) + "px";
			document.getElementById("SliderHandleGraphic_").style.width = (this.SliderHandleWidth) + "px"
			
			
			this.XRLimit = (Scrolly_.ScrollBarWidth -this.SliderHandleWidth)+21;
			
			
			

		
		}
		
		
