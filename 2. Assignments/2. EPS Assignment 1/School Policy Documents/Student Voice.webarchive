function frgcbb_get_x(arg_html_object,arg_window)
{
	if (!arg_html_object)
	{
		return 0;
	}

	if (arg_html_object.offsetParent)
	{
		var offset_parent = arg_html_object.offsetParent;
	}
	else
	{
		var offset_parent = false;
	}

//FIXME need to add scroll Left and scroll Top
	if (offset_parent)
	{
		// if this is in the menu wizard, we need to take scroll into account, so it's easiest to put the menu on the left-hand side
		var x = arg_window.document.getElementById("menu_div");
		if(x)
		{
			ret = 38;
		}
		else
		{
			ret = arg_html_object.offsetLeft + frgcbb_get_x(offset_parent,arg_window);
		}
		return ret;
	}
	else if (arg_window.name == '')
	{
		return 0;
	}
	else if(arg_window != glob_top_window)
	{
		var var_frame_object = arg_window.parent.document.getElementById(arg_window.name);
		var frames_arr=arg_window.parent.document.getElementsByName(arg_window.name);
		if (!var_frame_object && frames_arr[0])
		{
			var_frame_object = frames_arr[0];
		}
		return arg_html_object.offsetLeft + frgcbb_get_x(var_frame_object,arg_window.parent);
	}
	else
	{
		return arg_html_object.offsetLeft;
	}
	
}

function frgcbb_get_y(arg_html_object,arg_window)
{
	if (!arg_html_object)
	{
		return 0;
	}

	if (arg_html_object.offsetParent)
	{
		var offset_parent = arg_html_object.offsetParent;
	}
	else
	{
		var offset_parent = false;
	}

	var offset_parent = arg_html_object.offsetParent;
	if (offset_parent)
	{
		var ret = arg_html_object.offsetTop + frgcbb_get_y(offset_parent,arg_window);
		return ret;
	}
	else if (arg_window.name == '')
	{
		return 0;
	}
	else if(arg_window != glob_top_window)
	{
		var var_frame_object = arg_window.parent.document.getElementById(arg_window.name);
		var frames_arr=arg_window.parent.document.getElementsByName(arg_window.name);
		if (!var_frame_object && frames_arr[0])
		{
			var_frame_object = frames_arr[0];
		}
		return arg_html_object.offsetTop + frgcbb_get_y(var_frame_object,arg_window.parent);
	}else
	{
		return arg_html_object.offsetTop;
	}
	
}

