		function SetAlpha(s_Style,n_Value){
		
		
			if(StyleObj = getCSSRule(s_Style)){
			
			if(browser_.isNS){
		
	
		
				if(n_Value==100){
				
					StyleObj.style.opacity = 1;
				
				}else{
		
					StyleObj.style.opacity = "."+n_Value
				
				}
		
			
			}else{
	
	
					if(n_Value==100){
				
					StyleObj.style.filter = "";
				
				}else{
		
					StyleObj.style.filter = "alpha(opacity="+n_Value+")";
				
				}
					
				
			}
			
			}
			
		}
		
